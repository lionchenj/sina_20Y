{
   "background": {
      "page": "lib/chrome/background.html"
   },
   "browser_action": {
      "default_icon": "data/icon16.png",
      "default_title": "App for Instagram™"
   },
   "content_scripts": [ {
      "css": [ "data/inject.css" ],
      "matches": [ "*://*.instagram.com/*" ]
   } ],
   "description": "An easy-to-use and lite App for Instagram",
   "homepage_url": "http://mybrowseraddon.com/instagram-app.html",
   "icons": {
      "128": "data/icon128.png",
      "16": "data/icon16.png",
      "32": "data/icon32.png",
      "48": "data/icon48.png",
      "64": "data/icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "App for Instagram™",
   "permissions": [ "storage" ],
   "short_name": "appforinstagram",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0"
}
